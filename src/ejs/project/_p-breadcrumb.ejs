<div class="p-breadcrumb">
  <div class="p-breadcrumb__l-inner l-inner">
    <ol class="p-breadcrumb__items" itemscope itemtype="https://schema.org/BreadcrumbList">
      <li class="p-breadcrumb__item" itemprop="itemListElement" itemscope itemtype="https://schema.org/ListItem">
        <a itemprop="item" href="https://codeups-teamproject.hi-web-design.com/">
          <span itemprop="name">トップ</span>
        </a>
        <meta itemprop="position" content="1" />
      </li>
      <li class="p-breadcrumb__item" itemprop="itemListElement" itemscope itemtype="https://schema.org/ListItem">
        <a itemprop="item" href="<%= link %>">
          <span itemprop="name"><%= seccondPage %></span>
        </a>
        <meta itemprop="position" content="2" />
      </li>
      <% if( ! thirdPage == "" ) { %>
        <li class="p-breadcrumb__item" itemprop="itemListElement" itemscope itemtype="https://schema.org/ListItem">
          <a itemprop="item" href="<%= link3 %>">
            <span itemprop="name"><%= thirdPage %></span>
          </a>
          <meta itemprop="position" content="3" />
        </li>
      <% } %>
    </ol>
  </div>
</div>
